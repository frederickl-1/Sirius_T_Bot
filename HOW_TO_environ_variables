export KC_API_SECRET=""
export KC_API_PASS=""
export TWIL_ACCOUNT_SID='"
export TWIL_AUTH_TOKEN='"

# See environment variables
printenv | grep KC_API_KEY


# Need to adjust the crontab -e file as follows
